#include <stdio.h>

int main() {
   int n,i,j,k;
    printf("enter the number of rows:-");
    scanf("%d",&n);
    
    for(i=n;i>=1;i--){
        for(int j=1;j<=n-i;j++){
            printf(" ");
        }
        for(int k=j;k<=i;k++){
            printf("*");
        }
        printf("\n");
    }

    return 0;
}
